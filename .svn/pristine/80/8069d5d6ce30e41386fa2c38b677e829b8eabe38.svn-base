package com.example.model;
import org.springframework.stereotype.Repository;

import java.io.Serializable;

@Repository
public class News implements Serializable {

	private Integer fid;
	private String title;
	private String images_id;
	private Integer sort;
	private String record_status;
	private String create_time;
	private Integer creator_id;
	private String file_path;
	private String digest;
	private String content;
	private String class_id;

	public String getClass_name_zh() {
		return class_name_zh;
	}

	public void setClass_name_zh(String class_name_zh) {
		this.class_name_zh = class_name_zh;
	}

	private String class_name_zh;

	public Integer getFid() {
		return fid;
	}

	public void setFid(Integer fid) {
		this.fid = fid;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getImages_id() {
		return images_id;
	}

	public void setImages_id(String images_id) {
		this.images_id = images_id;
	}

	public Integer getSort() {
		return sort;
	}

	public void setSort(Integer sort) {
		this.sort = sort;
	}

	public String getRecord_status() {
		return record_status;
	}

	public void setRecord_status(String record_status) {
		this.record_status = record_status;
	}

	public String getCreate_time() {
		return create_time;
	}

	public void setCreate_time(String create_time) {
		this.create_time = create_time;
	}

	public Integer getCreator_id() {
		return creator_id;
	}

	public void setCreator_id(Integer creator_id) {
		this.creator_id = creator_id;
	}

	public String getFile_path() {
		return file_path;
	}

	public void setFile_path(String file_path) {
		this.file_path = file_path;
	}

	public String getDigest() {
		return digest;
	}

	public void setDigest(String digest) {
		this.digest = digest;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getClass_id() {
		return class_id;
	}

	public void setClass_id(String class_id) {
		this.class_id = class_id;
	}

	@Override
	public String toString() {
		return "News{" +
				"fid=" + fid +
				", title='" + title + '\'' +
				", images_id='" + images_id + '\'' +
				", sort=" + sort +
				", record_status='" + record_status + '\'' +
				", create_time='" + create_time + '\'' +
				", creator_id=" + creator_id +
				", file_path='" + file_path + '\'' +
				", digest='" + digest + '\'' +
				", content='" + content + '\'' +
				", class_id='" + class_id + '\'' +
				", class_name_zh='" + class_name_zh + '\'' +
				'}';
	}

	public News(){

	}

	public News(Integer fid, String title, String images_id, Integer sort, String record_status, String create_time, Integer creator_id, String file_path, String digest, String content, String class_id, String class_name_zh) {
		this.fid = fid;
		this.title = title;
		this.images_id = images_id;
		this.sort = sort;
		this.record_status = record_status;
		this.create_time = create_time;
		this.creator_id = creator_id;
		this.file_path = file_path;
		this.digest = digest;
		this.content = content;
		this.class_id = class_id;
		this.class_name_zh = class_name_zh;
	}

	public News(Integer fid, String title, String images_id, Integer sort, String record_status, String create_time, Integer creator_id, String file_path, String digest, String content, String class_id) {
		this.fid = fid;
		this.title = title;
		this.images_id = images_id;
		this.sort = sort;
		this.record_status = record_status;
		this.create_time = create_time;
		this.creator_id = creator_id;
		this.file_path = file_path;
		this.digest = digest;
		this.content = content;
		this.class_id = class_id;
	}
}
